{
  "RulesConfig": {
    "Rules": [
      {
        "DomainNamePattern": "^(.*)\\.box$",
        "Strategy": "Multicast",
        "IsEnabled": true,
        "QueryTimeout": 3000
      },
      {
        "DomainNamePattern": "^(.*)\\.amazonaws\\.com$",
        "Strategy": "DoH",
        "NameServer": [
          "https://cloudflare-dns.com/dns-query"
        ],
        "IsEnabled": true,
        "QueryTimeout": 1000
      },
      {
        "DomainName": "www.google.de",
        "NameServer": [
          "8.8.8.8",
          "8.8.4.4"
        ],
        "Strategy": "Dns",
        "IsEnabled": true,
        "CompressionMutation": true,
        "QueryTimeout": 3000
      },
      {
        "DomainNamePattern": "",
        "DomainName": "",
        "NameServer": [
          ""
        ],
        "IpAddress": "",
        // Dns, DoH, Multicast
        "Strategy": "",
        "IsEnabled": true,
        "CompressionMutation": false,
        "QueryTimeout": 3000
      }
    ]
  }
}