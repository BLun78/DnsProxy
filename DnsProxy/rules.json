{
  "RulesConfig": {
    "Rules": [
      {
        "DomainNamePattern": "(.*)\\.box",
        "Strategy": "Multicast",
        "IsEnabled": false,
        "QueryTimeout": 30000
      },
      {
        "DomainNamePattern": "(.*)\\.amazonaws.com.",
        "Strategy": "DoH",
        "NameServer": [
          "https://cloudflare-dns.com/dns-query"
        ],
        "IsEnabled": true,
        "QueryTimeout": 30000
      },
      {
        "DomainNamePattern": "(.*)\\.blun.de.",
        "Strategy": "DoH",
        "NameServer": [
          "https://cloudflare-dns.com/dns-query"
        ],
        "IsEnabled": true,
        "QueryTimeout": 30000
      },
      {
        "DomainNamePattern": "blun.de.",
        "Strategy": "DoH",
        "NameServer": [
          "https://cloudflare-dns.com/dns-query"
        ],
        "IsEnabled": true,
        "QueryTimeout": 30000
      },
      {
        "DomainName": "www.google.de",
        "NameServer": [
          "8.8.8.8",
          "8.8.4.4"
        ],
        "Strategy": "Dns",
        "IsEnabled": true,
        "CompressionMutation": true,
        "QueryTimeout": 30000
      },
      {
        "DomainNamePattern": "",
        "DomainName": "",
        "NameServer": [
          ""
        ],
        "IpAddress": "",
        // Dns, DoH, Multicast
        "Strategy": "",
        "IsEnabled": false,
        "CompressionMutation": false,
        "QueryTimeout": 30000
      }
    ]
  }
}